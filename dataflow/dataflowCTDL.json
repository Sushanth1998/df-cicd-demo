{
	"name": "dataflowCTDL",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Datasetsql",
						"type": "DatasetReference"
					},
					"name": "sql"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Dataset_csv",
						"type": "DatasetReference"
					},
					"name": "storage"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     Size as string",
				"}",
				"source(output(",
				"          UserId as integer,",
				"          ProductId as integer,",
				"          ItemsPurchasedLast12Months as integer,",
				"          IsTopProduct as boolean,",
				"          IsPreferredProduct as boolean",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> sql",
				"sql filter(!isNull($Size)) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          UserName as string,",
				"          Gender as string,",
				"          Phone as string,",
				"          Email as string,",
				"          CreditCard as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> storage"
			]
		}
	}
}